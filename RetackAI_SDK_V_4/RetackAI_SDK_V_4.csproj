<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>RetackAI</PackageId>
    <PackageId>RetackAI.AspNetSdk.NuGet</PackageId>
    <Title>Retack-AI</Title>
    <Version>1.0.0</Version>
    <Authors>Truenary</Authors>
    <Company>Truenary Solution Pvt. Ltd.</Company>
