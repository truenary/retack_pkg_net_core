    <PackageIcon>RetackLogo.png</PackageIcon>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <EnableDefaultCompileItems>false</EnableDefaultCompileItems>
